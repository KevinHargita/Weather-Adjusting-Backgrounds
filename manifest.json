{
  "name": "Weather Adjusting Backgrounds",
  "description": "Adjust Google Hangouts background based on local weather and time of day.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
    "matches": ["*://meet.google.com/*"],
    "js": ["app.js"],
    "run_at": "document_start"
    }
  ],
  "host_permissions": [
    "https://meet.google.com/*"
  ],
  "permissions": ["storage"]
}
